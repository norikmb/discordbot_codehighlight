{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": ["airbnb-base", "prettier"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 2021,
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"ignorePatterns": ["dest/**/*"],
	"rules": { "no-console": "off" }
}
